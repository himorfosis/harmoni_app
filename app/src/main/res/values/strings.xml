    <!-- about app harmoni -->
    <string name="our_story">Our Story</string>
    <string name="our_story_description">Berawal dari sebuah perbincangan di whatsapp pada tahun 2015. Kami berkembang menjadi sebuah perusahaan solid yang
        bergerak di bidang Event Organizer dan Multimedia Services.

        Hingga tahun 2019, kami sudah menangani lebih dari 145
        event, mulai dari pulau Sumatra, Batam, Jawa, Kalimantan,
        Bali, Lombok, Sulawesi, Maluku, hingga Papua.</string>

    <string name="our_vision">Our Vision</string>
    <string name="our_vision_description">
